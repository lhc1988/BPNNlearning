<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context-3.0.xsd ">

    
    <bean id="dao" class="lab.cgcl.aliBigdata.BPNNlearning.dao.SimpleDao">
    </bean>
    
    <bean id="wrapdao" class="lab.cgcl.aliBigdata.BPNNlearning.dao.WrapedDao">
    </bean>
    
    <bean id="bp" class="lab.cgcl.aliBigdata.BPNNlearning.BP">
    </bean>
    
    <bean id="parameterpool" class="lab.cgcl.aliBigdata.BPNNlearning.ParameterPool">
    	<property name="dao">
	    <ref bean="wrapdao" />
	    </property>
    </bean>
    
    <bean id="get0" class="lab.cgcl.aliBigdata.BPNNlearning.getVector.Get00">
    	<property name="dao">
	    <ref bean="dao" />
	   </property>
    </bean>
 </beans>